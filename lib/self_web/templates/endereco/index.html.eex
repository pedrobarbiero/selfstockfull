<div class="default-form"> 
  <h1>Endere√ßos</h1>
</div>
<div class="default-form"> 
  <table>
    <thead>
      <tr>
        <th>Logradouro</th>
        <th>Numero</th>
        <th>Cep</th>
        <th>Bairro</th>

        <th></th>
      </tr>
    </thead>
    <tbody>
  <%= for endereco <- @enderecos do %>
      <tr>
        <td><%= endereco.logradouro %></td>
        <td><%= endereco.numero %></td>
        <td><%= endereco.cep %></td>
        <td><%= endereco.bairro %></td>

        <td>
          <%= link "Exibir", to: Routes.endereco_path(@conn, :show, endereco) %>
          <%= link "Editar", to: Routes.endereco_path(@conn, :edit, endereco) %>
          <%= link "Excluir", to: Routes.endereco_path(@conn, :delete, endereco), method: :delete, data: [confirm: "Certeza?"] %>
        </td>
      </tr>
  <% end %>
    </tbody>
  </table>

  <span><%= link "Novo", to: Routes.endereco_path(@conn, :new) %></span>
</div>
